    "@types/express-serve-static-core": "^4.17.12",
    "@types/jest": "^28.1.6",
    "@types/node": "^16.0.0",
    "@types/passport": "^1.0.6",
    "@types/pg": "^8.6.0",
    "@types/supertest": "^2.0.12",
    "@types/unzipper": "^0.10.4",
    "aws-sdk-client-mock": "^2.0.0",
    "aws-sdk-client-mock-jest": "^2.0.0",
    "esbuild": "^0.15.13",
    "esbuild-runner": "^2.2.2",
    "jest": "^28.1.3",
    "nodemon": "^2.0.7",
    "supertest": "^6.2.4",
    "ts-jest": "^28.0.7",
    "tsconfig-paths": "^3.9.0"
  },
