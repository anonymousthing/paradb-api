    "@types/zxcvbn": "^4.4.1",
    "bcrypt": "^5.0.1",
    "camelcase-keys-recursive": "^1.0.0",
    "cookie-parser": "^1.4.5",
    "cookie-session": "^1.4.0",
    "dotenv": "^10.0.0",
    "encoding": "^0.1.13",
    "express": "^4.17.12",
    "meilisearch": "^0.29.1",
    "paradb-api-schema": "file:../paradb-api-schema",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "pg": "^8.6.0",
    "snakecase-keys": "^4.0.2",
    "typescript": "^4.9.1-beta",
